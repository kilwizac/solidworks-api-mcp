  "title": "IDimension.GetSystemValue3",
  "description": "Gets the value of the current dimension in system units in the named configuration.",
  "signature": "System.Object GetSystemValue3( \n   System.Int32\nWhichConfigurations\n,\n   System.Object\nConfig_names\n)",
  "parameters": [
    {
      "name": "WhichConfigurations",
      "type": "System.Int32",
      "description": "Configurations to get this value from as defined by swInConfigurationOpts_e (see Remarks )"
    },
    {
      "name": "Config_names",
      "type": "System.Object",
      "description": "Names of the configuration (see Remarks )"
    }
  ],
  "returns": "Value in system units",
  "remarks": "The Config_names argument is only used if whichConfigurations is set to swSpecifyConfiguration.\nIn OLE, Config_names can be either a BSTR array or a single BSTR.",
