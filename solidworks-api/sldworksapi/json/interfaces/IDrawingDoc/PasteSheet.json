  "title": "IDrawingDoc.PasteSheet",
  "description": "Copies and pastes a drawing sheet to the specified location of the drawing document, optionally renaming whenever duplicate names occur.",
  "signature": "System.Boolean PasteSheet( \n   System.Int32\nInsertOption\n,\n   System.Int32\nRenameOption\n)",
  "parameters": [
    {
      "name": "InsertOption",
      "type": "System.Int32",
      "description": "Option as defined in swInsertOptions_e"
    },
    {
      "name": "RenameOption",
      "type": "System.Int32",
      "description": "Option as defined in swRenameOptions_e ; 1 to rename duplicate section, detail or auxiliary view names; 2 to not rename"
    }
  ],
  "returns": "True if successful, false if not",
  "remarks": "Before calling this method, you must:\nSelect a sheet.\nCall\nIModelDoc2::EditCopy\n.",
